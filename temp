kubectl config set-cluster saas-rnd-cap-01 \
    --certificate-authority=${HOME}/.kube/certs/saas-rnd-cap-01/k8s-ca.crt \
    --server=https://api.saas-rnd-cap-01.managed.netcracker.cloud:6443

kubectl config set-credentials nurtay.tolymbek-saas-rnd-cap-01 \
    --auth-provider=oidc \
